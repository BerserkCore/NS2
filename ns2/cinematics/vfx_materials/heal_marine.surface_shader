<shader>	<inputs>		<input name="noiseMap" type="texture" address="wrap" />		<input name="particles" type="texture" address="wrap" />		<input name="timeLastHealed" type="float" />	</inputs>		<params>		<param name="twoSided">false</param>		<param name="alphaTest">false</param>	</params>		<code>	<![CDATA[		void MaterialShader(in Material_Input input, inout Material material)		{			const float3 tint = float3(0.02, 0.02, .3);			const float animationDuration = 2;			const float pulseAnimationDuration = 0.6;			            float t = time - timeLastHealed;            float animFraction = 1 - clamp(t / animationDuration, 0, 1);			float pulseAnimFraction = 1 - clamp(t / pulseAnimationDuration, 0, 1);				float3 noise = tex2D(particles, input.texCoord * 15 + float2(time * 1, 1.3 * time)).rgb; 			float3 circuit = tex2D(noiseMap, input.texCoord * 5 + float2(2, 2)).rgb; 			material.emissive = (circuit * tint + pow(circuit * noise, 2.3) * 5 * pulseAnimFraction * float3(0.5, 0.8, 1)) * animFraction;		}	]]>	</code></shader>